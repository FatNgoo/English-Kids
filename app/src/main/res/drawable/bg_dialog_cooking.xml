<?xml version="1.0" encoding="utf-8"?>
<!-- Dialog background - Warm cooking theme -->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <corners android:radius="24dp" />
    <gradient
        android:angle="135"
        android:startColor="#FFFDE7"
        android:centerColor="#FFF8E1"
        android:endColor="#FFECB3"
        android:type="linear" />
    <stroke
        android:width="3dp"
        android:color="#FFB74D" />
</shape>
